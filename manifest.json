{
    "name": "CovidSearch",
    "description": "Extension that provides a location's covid guidelines",
    "version": "1.0",
    "manifest_version": 3,
    
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo1.png"
      },

    "commands": {
        "_execute_action": {
        "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens CovidSearch.html"
        }
    }
  }

